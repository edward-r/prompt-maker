{
  "name": "@perceptron/prompt-maker-cli",
  "version": "0.1.0",
  "description": "Terminal workflow for diagnosing and improving prompts via prompt-maker.",
  "license": "MIT",
  "type": "module",
  "bin": {
    "prompt-maker-cli": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node scripts/build.mjs",
    "dev": "node scripts/dev.mjs",
    "start": "node dist/index.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "jest",
    "format": "prettier -w .",
    "lint": "prettier -c \"{src,packages,scripts,tests}/**/*.{ts,tsx,js,jsx,cjs,mjs,json,md}\"",
    "refresh": "npm run lint && npm run typecheck && npm test && npm run build && npm install -g .",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "prompt",
    "cli",
    "prompt-maker",
    "neovim"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "cli-cursor": "^4.0.0",
    "cli-table3": "^0.6.5",
    "clipboardy": "^5.0.1",
    "enquirer": "^2.4.1",
    "fast-glob": "^3.3.3",
    "html-to-text": "^9.0.5",
    "ink": "^4.4.1",
    "js-tiktoken": "^1.0.21",
    "js-yaml": "^4.1.0",
    "open": "^11.0.0",
    "ora": "^8.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "wrap-ansi": "^8.1.0",
    "yargs": "^18.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/react": "^16.1.0",
    "@types/html-to-text": "^9.0.4",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "esbuild": "^0.24.2",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  }
}
